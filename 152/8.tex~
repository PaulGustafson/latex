%\cfoot{}

\title{Quiz
%\author{Good Luck}
%\date{}

\begin{document}

\maketitle
\thispagestyle{fancy}

\vskip0.25in
\begin{enumerate}


\item[\textbf{1. }] (3 points) Find the antiderivative of the function $f(x) = x \sin(x^2)$.

\vskip 2in

\item[\textbf{2. }](3 points) Calculate  $\displaystyle \int_0^1 x^2 \sqrt{x + 5} \, dx$.
 
\vskip 2in

\item[\textbf{3. }] (4 points) Find the area of the region bounded on the left by the line $x = 1$, on the right by $x = 4$, above by $y = e^x$, and below by $y = x$.


\end{enumerate}
\end{document} 
